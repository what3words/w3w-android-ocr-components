    <string name="scan_state_detecting">Iyafumanisa...</string>
    <!-- it is a what3words address that is being scanned -->
    <string name="scan_state_found">Ifumanekile</string>
    <!-- This text shows up when the user is scanning a what3words address and the system is loading the results -->
    <string name="scan_state_loading">Iyakhangela...</string>
    <string name="scan_state_scanning">Khangela idilesi yamagama ama-3</string>
